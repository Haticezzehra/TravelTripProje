@using TravelTripProje.Models.Siniflar
@model List<Blog>
@{
   
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Blog Lisetesi</h2>
<table class="table table-bordered" >
    <tr>
        <th>ID</th>
        <th>Baslik</th>
      
        <th>Sil</th>
        <th>Güncelle</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var x in Model)
    {
<tr>
    <td>@x.ID</td>
    <td>@x.Baslik</td>
    <td><a href="#" class="btn btn-warning">Sil</a></td>


    <td><a href="#" class="btn btn-danger">Güncelle</a></td>
    <td><a href="#" class="btn btn-default">Detaylar</a></td>



</tr>
    }
</table>
<a href="/Admin/YeniBlog/" class="btn btn-primary">Yeni Blog</a>

